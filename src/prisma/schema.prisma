generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Vehicles {
  id          String   @id @default(uuid())
  name        String   @unique
  description String
  plate       String
  isFavorite  Boolean  @default(false)
  year        Int
  color       String
  price       Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @default(now())
}
